#include <iostream>

using namespace std;
class node{
    public:
    int data;
    node* next;
    node(int d)
    {
        data=d;
        next=NULL;
    }
};
void insertdata(node*&head,int data)
{
    node*n=new node(data);
    n->next=head;
    head=n;
}
void print(node*head)
{
    node*temp=head;
    while(temp!=NULL)
    {
        cout<<temp->data<<"->";
        temp=temp->next;
    }
    cout<<endl;
}
int main() {
    node*head=NULL;
    insertdata(head,5);
    insertdata(head,4);
    insertdata(head,3);
    print(head);
}
